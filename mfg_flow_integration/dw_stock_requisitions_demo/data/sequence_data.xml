<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_requisition_number" model="ir.sequence">
            <field name="name">Requisition Number</field>
            <field name="code">stock.picking.requisition</field>
            <field name="prefix">REQ</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_dw_mrp_requisition" model="ir.sequence">
            <field name="name">Manufacturing Requisition</field>
            <field name="code">dw.mrp.requisition</field>
            <field name="prefix">MREQ/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
